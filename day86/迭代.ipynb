    "import numpy as np\n",
    "arr=np.arange(12).reshape(4,3)\n",
    "print(arr)\n",
    "print('\\n')\n",
    "for x in np.nditer(arr.T):\n",
    "    print(x,end=',')\n",
    "print('\\n')\n",
    "\n",
    "print('-'*20)\n",
    "\n",
    "\n",
    "b=arr.T.copy(order='C')\n",
    "print(b)\n",
    "print('\\n')\n",
    "for x in np.nditer(b):\n",
    "    print(x,end=',')"
   ]
  }
